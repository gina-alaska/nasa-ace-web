- meta workspace: @workspace.id, view: @view.id, channel_key: SecureRandom.hex(10), map_view: @view.view_3d_mode ? '3d' : '2d'
#workspace-map.map-container.auto-collapse
  .map-toolbar
    = render 'map_toolbar'
  .map-sidebar{ data: { behavior: 'hover-toggle', target: '#workspace-map', class: 'expand' } }
    = render 'map_sidebar'
  .map-content
    #map.map{ data: { center: [@view.center_lng.to_f, @view.center_lat.to_f], zoom: @view.zoom } }
    %pre{ id: 'info' }
